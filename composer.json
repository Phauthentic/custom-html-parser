{
  "name": "phauthentic/custom-html",
  "license": "GPL",
  "authors": [
    {
      "name": "Florian Kr√§mer",
      "email": "florian.kraemer@kreative-design.net"
    }
  ],
  "require": {
      "ext-dom": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11.16",
    "phpunit/phpunit": "^8.3",
    "psr/cache": "^1.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Phauthentic\\CustomHtml\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phauthentic\\CustomHtml\\Test\\": "tests\\TestCase"
    }
  },
  "scripts": {
    "test": [
      ".\\bin\\phpunit"
    ],
    "cscheck": [
      ".\\bin\\phpcs .\\src .\\tests"
    ],
    "csfix": [
      ".\\bin\\phpcbf .\\src .\\tests"
    ],
    "analyze": [
      ".\\bin\\phpstan analyse -l 1 -c phpstan.neon .\\src\\"
    ]
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "prestissimo": {
      "maxConnections": 12,
      "minConnections": 6
    }
  }
}
